{
  "stateless": {
    "name": "stateless-group",
    "capacity": 100,
    "rules": [
      {
        "priority": 1,
        "action": "aws:pass",
        "match_attributes": {
          "protocol": 6,
          "source": "10.0.0.0/16",
          "destination": "192.168.1.0/24",
          "destination_ports": {
            "from": 80,
            "to": 80
          }
        }
      }
    ]
  },

  "stateful": {
  "name": "stateful-group",
  "capacity": 200,
  "rules": "pass tcp 10.0.0.0/16 [0:65535] -> 192.168.1.0/24 80 (sid:1;)"
}


}

